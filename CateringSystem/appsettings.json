{
  "Authentication": {
    "JwtKey": "PRIVATE_KEY_DONT_SHARE", //reprezentuje klucz potrzebny do wygenerowania tokenu JWT
    "JwtExpireDays": 15,
    "JwtIssuer": "http://cateringapi.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "myConn": "Server=localhost;Database=CateringDb;Trusted_Connection=True;"
  },
  "AllowedHosts": "*",
  "DeepLSettings": {
    "AuthorizationKey": { AuthorizationKey }
  }
}
